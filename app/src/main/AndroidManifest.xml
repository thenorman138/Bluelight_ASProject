<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.utc.bluelight">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".pinActivity" />
        <activity android:name=".choiceActivity" />
        <activity android:name=".walkActivity" />
        <activity android:name=".rideActivity" />
        <activity android:name=".rideConfirm" />
        <activity android:name=".addFriend" />
        <activity android:name=".TermsActivity"></activity>
    </application>

</manifest>